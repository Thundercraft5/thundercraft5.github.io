#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules/lerna/dist/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules/lerna/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules/lerna/dist/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules/lerna/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/lerna@8.2.2_encoding@0.1.13/node_modules:/mnt/s/__RSOD2/Noahs Rete de Sui, Omnis, et Deo/6. Opuses/Operonomes/Techspace/Repositories - Personal/thundercraft5.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lerna/dist/cli.js" "$@"
else
  exec node  "$basedir/../lerna/dist/cli.js" "$@"
fi
